{
  "type": "library",
  "readme": "README.md",
  "name": "jorisvandesande/yuki-api-client",
  "description": "An API client for the Yuki SOAP API endpoints",
  "keywords": ["yuki", "yukiworks", "soap", "api", "api client"],
  "license": "MIT",
  "homepage": "https://github.com/jorisvandesande/yuki-api-client",
  "authors": [
    {
      "name": "Joris van de Sande",
      "email": "joris@vandesande.link",
      "homepage": "https://github.com/jorisvandesande"
    }
  ],
  "require": {
    "php": "^7.1.3|^8.0",
    "ext-simplexml": "*",
    "ext-soap": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.1.3"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  },
  "autoload": {
    "psr-4": {
      "JVDS\\YukiClient\\": "src/"
    }
  }
}
